{
    "name": "alist heroku",
    "description": "Deploy alist on Heroku. alist running data is placed on the reclone mount disk  ",
    "keywords": ["alist"],
    "env": {
        "rclone": {
            "description": "$HOME/.config/rclone/rclone.conf",
            "value": "",
            "required": true
        },
        "parameter": {
            "description": "The following is just an example,Please modify it yourself",
            "value": "gd:up --buffer-size 64M --umask 0000 --default-permissions --allow-non-empty --allow-other --dir-cache-time 12h --vfs-cache-mode writes >/dev/null 2>&1 &",
            "required": true
        },
        "alist": {
            "description": "$HOME/alist/config/config.json",
            "value": "",
            "required": true
        }      
    },
    "stack": "container"
}
